{
  "name": "whats-this",
  "version": "0.0.1",
  "description": "TJ and Pals - What's This",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "start": "npm-run-all -p _webpack:dev _localcdn",
    "bundle": "npm run clean && npm run _webpack:release && npm run _copy:static",
    "_webpack:dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --progress --open --config webpack.dev.js",
    "_webpack:release": "cross-env NODE_ENV=production webpack --mode production --progress --config webpack.release.js",
    "_copy:static": "cpy * ../dist --cwd=_static && cpy **/* ../../dist/media --cwd=_static/media --parents",
    "_localcdn": "node ./build-utils/localcdn.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TJ-and-Pals/whats-this.git"
  },
  "author": "TJ and Pals Media Inc.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/TJ-and-Pals/whats-this/issues"
  },
  "homepage": "https://github.com/TJ-and-Pals/whats-this#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "cors": "^2.8.5",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "fluture": "^11.0.3",
    "fluture-loaders": "^3.0.2",
    "fp-ts": "^2.0.5",
    "howler": "^2.1.2",
    "lit-html": "^1.1.2",
    "partial.lenses": "^14.17.0",
    "xstate": "^4.6.7"
  }
}
